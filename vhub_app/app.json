{
  "expo": {
    "name": "vhub_app",
    "slug": "vhub_app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "assetBundlePatterns": [
      "**/*"
    ],
    "splash": {
      "hideExponentSplash": true,
      "image": "./src/assets/images/white_splashscreen.png",
      "resizeMode": "stretch",
      "backgroundColor": "#ffffff",
      "statusBar": {
        "style": "none"
      },
      "androidStatusBar": {
        "backgroundColor": "#FFFFFF",
        "barStyle": "dark-content"
      }
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-font"
    ]
  }
}